{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/eros/Projects/Learning/SpringBoot/SpringAI/mcp_dir"
